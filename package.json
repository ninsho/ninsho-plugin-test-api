{
  "name": "ninsho-plugin-admin-api",
  "version": "0.0.1",
  "description": "npm module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "lint": "eslint \"src/**/*.ts\" \"src/*.ts\"",
    "test": "jest --config ./jest.config.json --runInBand --coverage",
    "build-t": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "dev": "ts-node-dev --respawn src/x-dev.ts dev"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninsho/ninsho-plugin-test-api/issues"
  },
  "homepage": "https://github.com/ninsho/ninsho-plugin-test-api/",
  "dependencies": {
    "dotenv": "^16.0.3",
    "ninsho": "^0.0.14",
    "pg": "^8.10.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/pg": "^8.10.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0"
  }
}
